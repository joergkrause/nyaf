{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@nyaf Library API Documentation",  
  "compilerOptions": {
    "ignoreCompilerErrors": true,
    "includeDeclarations": false,
    "mode": "file",    
    "preserveConstEnums": true,
    "jsx": "react",
    "target": "ES2015",
    "excludePrivate": true,
    "excludeNotExported": true,
    "excludeExternals": false,
  },
  "theme": "default",
  "out": "dist/apidoc/lib",
  "entryPoints": ["./src/packages/@nyaf/lib/index.ts"],
  "exclude": [
    "./src/_experimental"
  ]  
}
