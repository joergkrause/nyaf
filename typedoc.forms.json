{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@nyaf Forms API Documentation",
  "compilerOptions": {
    "ignoreCompilerErrors": true,
    "includeDeclarations": false,
    "mode": "file",    
    "preserveConstEnums": "true",
    "excludePrivate": true,
    "excludeNotExported": true,
    "excludeExternals": false    
  },  
  "out": "dist/apidoc/forms",
  "theme": "default",
  "entryPoints": ["./src/packages/@nyaf/forms/index.ts"],        
  "exclude": [
    "./src/_experimental"
  ]
}
